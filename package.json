    "@nebula.js/cli-sense": "5.8.1",
    "@playwright/test": "^1.28.1",
    "@qlik/sdk": "^0.12.0",
    "eslint": "7.27.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-mocha": "9.0.0"
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=5.0.0"
  }
